{
    "name": "@passage/query-passage",
    "version": "0.0.3",
    "description": "query-sparql + Passage for continuation queries",
    "license": "MIT",
    "sideEffects": false,
    "main": "lib/index.js",
    "typings": "lib/index",
    "publishConfig": {
        "access": "public"
    },
    "bin": {
        "comunica-passage": "bin/query.js"
    },
    "files": [
        "bin/**/*.d.ts",
        "bin/**/*.js",
        "bin/**/*.js.map",
        "components",
        "config",
        "engine-default.js",
        "lib/**/*.d.ts",
        "lib/**/*.js",
        "lib/**/*.js.map"
    ],
    "scripts": {
        "build": "yarn run build:ts",
        "build:ts": "node \"../../node_modules/typescript/bin/tsc\"",
        "prepare": "comunica-compile-config config/config-default.json > engine-default.js"
    },
    "dependencies": {
        "@comunica/config-query-sparql": "^4.1.0",
        "@comunica/query-sparql": "^4.1.0",
        "@comunica/runner-cli": "^4.1.0",
        "@passage/actor-query-source-identify-hypermedia-passage": "^0.0.3",
        "@passage/config-query-passage": "^0.0.3"
    },
    "browser": {
        "./lib/index.js": "./lib/index-browser.js"
    }
}
